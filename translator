    {
      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "from googletrans import Translator\n",
        "import time\n",
        "\n",
        "# Load your JSON dataset into a pandas DataFrame\n",
        "# Replace 'your_dataset.json' with the actual file path\n",
        "df = pd.read_json('/content/drive/MyDrive/data/val.json')\n",
        "\n",
        "# Create a Translator object\n",
        "translator = Translator()\n",
        "\n",
        "# Define a function to translate a text to Vietnamese\n",
        "def translate_to_vietnamese(text):\n",
        "    translation = translator.translate(text, dest='vi')\n",
        "    time.sleep(1)\n",
        "    return translation.text\n",
        "\n",
        "# Apply the translation function to the 'dialogue' and 'summary' columns\n",
        "df['dialogue_vi'] = df['dialogue'].apply(translate_to_vietnamese)\n",
        "df['summary_vi'] = df['summary'].apply(translate_to_vietnamese)\n",
        "\n",
        "# Display the translated DataFrame\n",
        "print(df.head())\n"
      ],
